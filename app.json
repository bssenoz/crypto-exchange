{
  "expo": {
    "name": "Kripto Borsam",
    "slug": "Kripto Borsam",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/AppIcon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/AppIcon.png",
      "resizeMode": "contain",
      "backgroundColor": "#14181b"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "Uygulama, arkadaşlarınızla paylaşmanıza olanak sağlamak için fotoğraflarınıza erişir.",
          "cameraPermission": "Uygulama kameranızı açar."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/AppIcon.png",
          "color": "#ffffff"
        }
      ]
    ],
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/AppIcon.png",
        "backgroundColor": "#14181b"
      },
      "permissions": ["android.permission.RECORD_AUDIO"],
      "package": "com.walla121.CryptoTrackerFirebase"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "406ffe02-7abc-4378-99e6-bcdfc6483415"
      }
    }
  }
}
